{
  "name": "cesium-lite",
  "version": "1.2.0",
  "main": "index.js",
  "description": "CesiumLite是一款开箱即用的轻量化三维地图包,基于cesium开发,旨在封装常见的地图功能模块，便于在多个前端项目中复用。",
  "author": "lukeSuperCoder",
  "license": "MIT",
  "homepage": "https://github.com/lukeSuperCoder/cesium-lite",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukeSuperCoder/cesium-lite.git"
  },
  "keywords": [
    "cesium",
    "gis",
    "map",
    "3d"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite build && vite preview",
    "fix-paths": "node scripts/fix-paths.js",
    "predeploy": "npm run build",
    "deploy": "npm run fix-paths && gh-pages -d dist",
    "test": "npm run build",
    "prepublishOnly": "npm run build",
    "release:patch": "bash scripts/publish.sh patch",
    "release:minor": "bash scripts/publish.sh minor",
    "release:major": "bash scripts/publish.sh major"
  },
  "devDependencies": {
    "cesium": "^1.127.0",
    "gh-pages": "^6.3.0",
    "vite": "^2.5.6",
    "vite-plugin-cesium": "^1.2.9"
  },
  "dependencies": {
    "shpjs": "^6.1.0",
    "terraformer-wkt-parser": "^1.2.1"
  }
}
